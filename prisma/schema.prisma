// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  // Clerk provided ID
  id       String    @id @unique
  // Programs someone is following
  programs Program[]
}

model Program {
  // Provided my API
  id   Int    @id @unique
  User User[]
}
